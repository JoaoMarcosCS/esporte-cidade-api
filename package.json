{
  "name": "esporte-cidade-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "testOnly": "cross-env NODE_ENV=test node --test-only --import tsx src/tests/enrollment.test.ts",
    "testSingleFile": "cross-env NODE_ENV=test node --test --import tsx src/tests/enrollment.test.ts",
    "test": "cross-env NODE_ENV=test node --test --import tsx src/tests/**/*.test.ts",
    "typeorm": "ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/populando-atendimentos -d src/database/config.ts",
    "dev": "nodemon --watch \"src/\" -r \"tsconfig-paths/register\" --exec \"ts-node src/server.ts\" -e ts --env-file .env",
    "migration": "npx ts-node -O {\\\"module\\\":\\\"commonjs\\\"} ./node_modules/typeorm/cli.js migration:run -d src/database/config.ts",
    "seed": "npx ts-node src/seeds/seedAtendiments.ts",
    "seedTest": "cross-env NODE_ENV=test npx ts-node src/seeds/seedAtendiments.ts",
    "test-jest": "jest --config jest.config.js",
    "test-jest:watch": "jest --watch",
    "test-jest:coverage": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "mysql2": "^3.11.3",
    "nodemon": "^3.1.7",
    "pdfkit": "^0.17.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.2",
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typeorm": "^0.3.22",
    "typescript": "^5.8.3"
  }
}
